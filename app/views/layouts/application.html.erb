<!DOCTYPE html>
<html>
<head>
  <title>Sayings</title>
  <%= stylesheet_link_tag "application" %>
  <script type="text/javascript" src="//use.typekit.net/uab3uxn.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrap">
    <div id="nav">
      <div id='account'>
      </div>
    </div>
    <div id="main">
      <div id="header">
	      <div id="notice"><%= notice %></div>
      	<div id="alert"><%= alert %></div>
      </div>
      <div id='user'></div>

      <%= yield %>
    </div>
  </div>
  <%= javascript_include_tag "application" %>
  <%= yield :javascript %>
</body>
</html>
